    "@types/react-router-dom": "^5.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-macros": "^2.7.1",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.8.0",
    "chartjs-plugin-annotation": "^0.5.7",
    "downshift": "^3.2.2",
