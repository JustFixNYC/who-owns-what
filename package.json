{
  "name": "who-owns-what",
  "version": "1.0.0",
  "description": "who owns what in nyc.",
  "main": "index.js",
  "scripts": {
    "install-all": "yarn && cd client && yarn",
    "start": "concurrently \"yarn server\" \"yarn client\"",
    "server": "nodemon server.js --exec babel-node",
    "client": "cd client && yarn start",
    "build": "concurrently \"yarn build-server\" \"yarn build-client\"",
    "build-server": "babel server -d dist",
    "build-client": "cd client && yarn --production=false && yarn run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "yarn build",
    "prettier": "prettier",
    "prettier:check": "prettier \"**/*.{ts,tsx,js,scss}\" --check",
    "prettier:fix": "prettier \"**/*.{ts,tsx,js,scss}\" --write",
    "lint": "yarn prettier:check"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "chart.js": "^2.8.0",
    "chartjs-plugin-annotation": "^0.5.7",
    "concurrently": "^3.5.0",
    "csv-express": "^1.2.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "heroku-ssl-redirect": "^0.0.4",
    "keen-tracking": "^1.1.4",
    "lodash": "^4.17.15",
    "morgan": "^1.8.2",
    "pg": "^6.4.0",
    "pg-promise": "^6.5.1",
    "prop-types": "15.7.2",
    "react": "^16.0.0",
    "react-chartjs-2": "^2.7.4",
    "react-dom": "^16.0.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "rollbar": "2.4.6"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "prettier": "2.0.5"
  }
}
